  "dependencies": {
    "@prisma/client": "^6.13.0",
    "bree": "^9.2.4",
    "cbor": "^10.0.9",
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "dotenv-cli": "^10.0.0",
    "express": "^5.1.0",
    "lodash": "^4.17.21",
    "lowdb": "^7.0.1",
    "moment-timezone": "^0.6.0",
    "node-schedule": "^2.1.1",
    "winston": "^3.17.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@types/bun": "latest",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
