    "@types/node-schedule": "^2.1.8",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "eslint": "^9.32.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.13.0",
    "ts-essentials": "^10.1.1",
    "ts-node": "^10.9.2",
    "typescript": "5.9.2"
  }
